function App:OnKeySubmit(player, key, statusLabel, successCallback)
    if not self.keyTimeStore then
        statusLabel.Text = "Error: Key system is offline."
        warn("WalvyXploit: OnKeySubmit failed because DataStore is not available.")
        return
    end

    if key ~= CONFIG.CORRECT_KEY then
        statusLabel.Text = "Incorrect Key"
        return
    end

    statusLabel.Text = "Checking key..."

    local success, result = pcall(function()
        return self.keyTimeStore:GetAsync(player.UserId .. "_" .. CONFIG.CORRECT_KEY)
    end)

    if not success then
        statusLabel.Text = "Error: Could not verify key. Try again."
        warn("WalvyXploit: GetAsync failed. Error: " .. tostring(result))
        return
    end

    local activationTime = result
    local currentTime = _tick()

    if activationTime and (currentTime - activationTime < CONFIG.KEY_VALID_DURATION) then
        statusLabel.Text = "Key session active. Opening menu..."
        task.wait(1)
        successCallback()
    else
        -- This block runs if activationTime is nil (new key) or expired.
        statusLabel.Text = "Key is valid, activating..."
        local setSuccess, setError = pcall(function()
            self.keyTimeStore:SetAsync(player.UserId .. "_" .. CONFIG.CORRECT_KEY, currentTime)
        end)
        if setSuccess then
            statusLabel.Text = "Key activated! Opening menu..."
            task.wait(1)
            successCallback()
        else
            statusLabel.Text = "Error: Could not save key. Try again."
            warn("WalvyXploit: SetAsync failed. Error: " .. tostring(setError))
        end
    end
end

function App:CheckForExistingKeySession(player, successCallback)
    if not self.keyTimeStore then
        warn("WalvyXploit: CheckForExistingKeySession failed because DataStore is not available.")
        return
    end

    local success, result = pcall(function()
        return self.keyTimeStore:GetAsync(player.UserId .. "_" .. CONFIG.CORRECT_KEY)
    end)

    if not success then
        warn("WalvyXploit: Initial key check failed. Error: " .. tostring(result))
        return
    end

    local activationTime = result
    if activationTime and (_tick() - activationTime < CONFIG.KEY_VALID_DURATION) then
        successCallback()
    elseif activationTime then
        -- Key exists but is expired, remove it.
        local _, removeError = pcall(function()
            self.keyTimeStore:RemoveAsync(player.UserId .. "_" .. CONFIG.CORRECT_KEY)
        end)
        if removeError then
            warn("WalvyXploit: Failed to remove expired key. Error: " .. tostring(removeError))
        end
    end
end

