--[[
    Script: WalvyXploit GUI with Timed Key System
    Description: A Roblox GUI script featuring a key system that allows access for 8 hours per key activation.
                 This version includes improved error handling and debugging for the key system.
    Author: Walvy
    Version: 1.6 (Key System Hotfix)
]]

--================================================================--
-- Environment Compatibility & Polyfills
--================================================================--

local Players = game:GetService("Players")
local DataStoreService = game:GetService("DataStoreService")
local CoreGui = game:GetService("CoreGui")

local _tick = tick or os.time

local function safeSetClipboard(text)
    if typeof(setclipboard) == "function" then
        pcall(setclipboard, text)
    else
        warn("WalvyXploit: 'setclipboard' is not available in this environment.")
    end
end

