    CORRECT_KEY = "i",
    DISCORD_INVITE_LINK = "https://discord.gg/5MhMjmVA",
    KEY_VALID_DURATION = 288000,
    DATASTORE_NAME = "WalvyKeyActivationTimeStore, Alomanii404"
}

