--================================================================--
-- Environment Compatibility
--================================================================--

local function safeSetClipboard(text)
    if typeof(setclipboard) == "function" then
        pcall(setclipboard, text)
    else
        warn("WalvyXploit: 'setclipboard' is not available in this environment.")
    end
end

